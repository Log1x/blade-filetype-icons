{
  "name": "log1x/blade-filetype-icons",
  "description": "A package to easily make use of dmhendricks/file-icon-vectors in your Laravel Blade views.",
  "keywords": [
    "Blade",
    "Laravel",
    "File Icons",
    "File Type Icons"
  ],
  "homepage": "https://github.com/log1x/blade-filetype-icons",
  "license": "MIT",
  "authors": [
    {
      "name": "Brandon Nifong",
      "homepage": "https://github.com/log1x"
    }
  ],
  "require": {
    "php": "^8.0",
    "blade-ui-kit/blade-icons": "^1.1",
    "illuminate/support": "^11.0"
  },
  "require-dev": {
    "dmhendricks/file-icon-vectors": "dev-master",
    "laravel/pint": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "Log1x\\BladeFileTypeIcons\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Log1x\\BladeFileTypeIcons\\BladeFileTypeIconsProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
